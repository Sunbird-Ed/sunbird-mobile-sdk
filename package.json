{
  "name": "sunbird-sdk",
  "version": "0.0.1",
  "description": "Heart of the sunbird mobile app. Holds the imporatnat business logic including api calls and db operations.",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "cordova-plugin-advanced-http": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/core": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "cordova-plugin-advanced-http": "^2.0.2",
    "jest": "^23.6.0",
    "ts-node": ">=3.2.0 <8",
    "tsickle": "^0.27.2",
    "tslib": "^1.9.0",
    "typescript": "^2.5.0"
  },
  "dependencies": {
    "jsrsasign": "^8.0.12",
    "moment": "^2.23.0"
  },
  "jest": {
    "browser": false,
    "rootDir": "./",
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/?(*.)(spec).ts"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "babel-jest"
    },
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": []
  }
}
